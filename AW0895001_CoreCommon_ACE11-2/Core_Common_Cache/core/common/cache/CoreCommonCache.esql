*******************************************************************************/
CREATE FUNCTION init (IN properties CHARACTER, IN Cache CHARACTER)
	RETURNS BOOLEAN
	LANGUAGE JAVA 
	EXTERNAL NAME 
			"com.bancolombia.integracion.cache.ESQLCacheWrapper.init";
